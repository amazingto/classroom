<view class="register"><view class="content"><view class="header"><image src="../../static/logo1.png"></image></view><view class="main"><w-input bind:input="__e" vue-id="1" type="text" maxlength="11" placeholder="手机号" value="{{phoneData}}" data-event-opts="{{[['^input',[['__set_model',['','phoneData','$event',[]]]]]]}}" bind:__l="__l"></w-input><w-input class="vue-ref" vue-id="2" type="text" placeholder="验证码" isShowCode data-ref="runCode" value="{{verCode}}" data-event-opts="{{[['^setCode',[['getVerCode']]],['^input',[['__set_model',['','verCode','$event',[]]]]]]}}" bind:setCode="__e" bind:input="__e" bind:__l="__l"></w-input><view class="uni-list" style="margin-top:4px !important;"><view class="uni-list-cell"><picker value="{{index}}" range="{{majordata}}" range-key="majorName" data-event-opts="{{[['change',[['bindPickerChange',['$event']]]]]}}" bindchange="__e"><view class="uni-input">{{majordata[index].majorName}}</view></picker></view></view><w-input bind:input="__e" vue-id="3" type="password" placeholder="登录密码" isShowPass value="{{passData}}" data-event-opts="{{[['^input',[['__set_model',['','passData','$event',[]]]]]]}}" bind:__l="__l"></w-input></view><w-button vue-id="4" text="注 册" rotate="{{isRotate}}" data-event-opts="{{[['tap',[['startReg']]]]}}" bindtap="__e" bind:__l="__l"></w-button></view></view>