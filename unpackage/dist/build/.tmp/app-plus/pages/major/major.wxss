page {
  width: 100%;
  height: 100%;
}
uni-page-body {
  height: 100%;
  width: 100%;
}
.container {
  height: 100%;
  overflow: hidden;
  width: 100%;
}
.container .my-segmented {
  height: 5%;
  width: 100%;
  padding-top: 15rpx;
  max-height: 38px;
}
.container .uni-list {
  height: 95%;
  width: 100%;
  overflow-y: auto;
  margin-top: 20rpx;
}
.container .btn-plusempty {
  width: 110rpx;
  height: 110rpx;
  background: #0bc99d;
  position: fixed;
  bottom: 50rpx;
  right: 50rpx;
  border-radius: 100%;
  overflow: hidden;
  text-align: center;
  line-height: 110rpx;
}
.container .bottom-btn-hover {
  background: #0bc99d !important;
}
.container .plusempty-img {
  width: 50rpx;
  height: 50rpx;
  margin-top: 30rpx;
}
.container .tui-fab-box {
  bottom: 100rpx;
}

